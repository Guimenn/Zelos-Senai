{
	"name": "api-studdy",
	"version": "1.0.0",
	"description": "API do Sistema de Helpdesk SENAI",
	"main": "src/server.js",
	"type": "module",
	"scripts": {
		"start": "node src/server.js",
		"dev": "npx nodemon src/server.js",
		"start:prod": "NODE_ENV=production node src/server.js",
		"start:cluster": "NODE_ENV=production node src/cluster.js",
		"build": "npm run build:prisma",
		"build:prisma": "prisma generate",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:migrate:deploy": "prisma migrate deploy",
		"db:seed": "node scripts/seed.js",
		"db:seed:helpdesk": "node scripts/seed-helpdesk.js",
		"db:studio": "prisma studio",
		"db:reset": "prisma migrate reset",
		"db:status": "prisma migrate status",
		"db:validate": "prisma validate",
		"optimize": "prisma generate && prisma migrate deploy",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write src/",
		"check:format": "prettier --check src/"
	},
	"keywords": [
		"helpdesk",
		"senai",
		"api",
		"nodejs",
		"express",
		"prisma",
		"postgresql"
	],
	"author": "Equipe SENAI",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^6.15.0",
		"@supabase/supabase-js": "^2.57.0",
		"bcrypt": "^6.0.0",
		"bcryptjs": "^3.0.2",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.18.2",
		"express-rate-limit": "^7.3.1",
		"helmet": "^8.0.0",
		"ioredis": "^5.3.2",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.2",
		"node-cache": "^5.1.2",
		"rate-limit-redis": "^4.2.0",
		"uuid": "^11.1.0",
		"zod": "^3.25.7"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/multer": "^1.4.12",
		"@types/node": "^22.10.2",
		"eslint": "^9.15.0",
		"nodemon": "^3.1.10",
		"prettier": "^3.4.2",
		"prisma": "^6.15.0"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"prisma": {
		"seed": "node scripts/seed-helpdesk.js"
	}
}
